<% if(productos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Titulo</th> <th>Precio</th> <th>Foto</th> </tr>
            <%productos.forEach(function(productos) {%>
            <tr>
                <td><%=productos.title%></td>
                <td><%=productos.price%></td>
                <td><%=productos.thumbnail%></td>
            </tr>
            <%});%>
        </table>
    </div>
<% } else{ %>
    <h3 class="alert alert-danger">no se encontraron datos</h1>
<% } %>